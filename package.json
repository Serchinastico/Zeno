        "@typescript-eslint/parser": "7.1.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.20.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "obsidian": "latest",
        "prettier": "^3.2.5",
        "prettier-plugin-organize-imports": "^3.2.4",
        "tslib": "2.6.2",
        "typescript": "5.3.3"
